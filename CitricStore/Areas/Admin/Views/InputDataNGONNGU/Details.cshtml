}
<link rel="stylesheet" href="~/Content/bootstrap5/css/bootstrap.css" />
<link rel="stylesheet" href="~/Content/Input.css" />
<link href="~/Content/DetailsBank.css" rel="stylesheet" />
<div class="container thandulieu" >


    <h2 class="chitiet">DỮ LIỆU NGÔN NGỮ</h2>

    <h3 class="chitiet">CHI TIẾT</h3>


    <div class="platform">



        <strong>Tên ngôn ngữ: </strong>
